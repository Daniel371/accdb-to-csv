{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "electron/*": [
        "./electron/*"
      ],
      "modules/*": [
        "src/modules/*"
      ],
      "components/*": [
        "src/components/*"
      ],
      "assets/*": [
        "src/assets/*"
      ],
      "backend/*": [
        "src/backend/*"
      ],
      "contexts/*": [
        "src/contexts/*"
      ],
      "hooks/*": [
        "src/hooks/*"
      ],
      "types/*": [
        "src/types/*"
      ],
      "utils/*": [
        "src/utils/*"
      ],
      "theme/*": [
        "src/theme/*"
      ],
      "routes/*": [
        "src/routes/*"
      ],
      "svg/*": [
        "src/svg/*"
      ],
    },
  },
  "include": [
    "src/**/*",
    "./electron/**/*",
    ".eslintrc.cjs",
    "vite.config.ts",
    "src/window.d.ts",
  ],
  "exclude": [],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}